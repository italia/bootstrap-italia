{
  "name": "bootstrap-italia",
  "version": "0.9.1",
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "description": "Bootstrap Italia Ã¨ un tema Bootstrap 4 per la creazione di applicazioni web nel pieno rispetto delle Linee guida di design per i servizi web della PA",
  "scripts": {
    "start": "gulp serve",
    "build": "gulp build",
    "documentation-build": "bundle exec jekyll build",
    "documentation-serve": "bundle exec jekyll serve",
    "documentation-deploy-to-gh-pages": "bundle exec jekyll build --config _config.yml,_config_production.yml && gh-pages -d _gh_pages -m 'Update documentation [ci skip]'",
    "release": "node scripts/release",
    "bump-major": "npm version major -m \"Release %s\"",
    "bump-minor": "npm version minor -m \"Release %s\"",
    "bump-patch": "npm version patch -m \"Release %s\"",
    "preversion": "npm test",
    "version": "node scripts/config-sync && git add _config.yml",
    "postversion": "echo \"You can now publish your tag with 'git push --follow-tags'\"",
    "test": "echo \"No tests specified\"",
    "test-a11y": "npx pa11y-ci --sitemap http://localhost:4000/sitemap.xml",
    "test-a11y-one": "npx pa11y-ci"
  },
  "repository": "https://github.com/italia/bootstrap-italia",
  "author": "Presidenza del Consiglio dei Ministri",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "files": [
    "dist/",
    "src/"
  ],
  "devDependencies": {
    "@octokit/rest": "^15.2.6",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.24.1",
    "conventional-changelog": "^1.1.24",
    "cross-spawn": "^6.0.5",
    "fancy-log": "^1.3.2",
    "gh-pages": "^1.1.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-touch-cmd": "0.0.1",
    "gulp-uglify": "^3.0.0",
    "gulp-wrapper": "^1.0.0",
    "replace-in-file": "^3.4.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "pickadate": "^3.5.6",
    "popper.js": "^1.14.3"
  }
}
