{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "isIe": true,
    "define": true,
    "require": true,
    "options": true
  }
}
