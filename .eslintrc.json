{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "ignorePatterns": [
    "*.min.js",
    "*.d.ts"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "bootstrap": true,
    "isIe": true,
    "define": true,
    "require": true,
    "options": true,
    "createjs": true,
    "anchors": true,
    "ClipboardJS": true,
    "tsParticles": true
  }
}
